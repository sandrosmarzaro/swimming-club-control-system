<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sccs.controller.ReportControl">
   <children>
      <TableView fx:id="table" layoutX="23.0" layoutY="26.0" prefHeight="372.0" prefWidth="391.0">
        <columns>
          <TableColumn fx:id="quantityColumn" prefWidth="74.0" text="Quantity" />
          <TableColumn fx:id="vacanciesColumn" prefWidth="95.0" text="Vacancies" />
            <TableColumn fx:id="averageColumn" prefWidth="91.0" text="Average Age" />
            <TableColumn fx:id="dayColumn" prefWidth="129.0" text="Day Of The Week" />
        </columns>
      </TableView>
      <Button fx:id="printButton" layoutX="347.0" layoutY="432.0" mnemonicParsing="false" onAction="#handleClickPrint" prefHeight="30.0" prefWidth="65.0" text="Print" />
      <Spinner fx:id="minSpinner" editable="true" layoutX="87.0" layoutY="408.0" prefHeight="26.0" prefWidth="82.0" />
      <Spinner fx:id="maxSpinner" editable="true" layoutX="88.0" layoutY="446.0" prefHeight="26.0" prefWidth="82.0" />
      <Label layoutX="23.0" layoutY="412.0" text="Min Age:" />
      <Label layoutX="21.0" layoutY="450.0" prefHeight="18.0" prefWidth="65.0" text="Max Age:" />
      <Button fx:id="refreshButton" layoutX="237.0" layoutY="433.0" mnemonicParsing="false" onAction="#handleClickRefresh" prefHeight="30.0" prefWidth="65.0" text="Refresh" />
   </children>
</AnchorPane>
